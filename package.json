        "fs-extra": "^8.1.0",
        "minimist": "^1.2.3",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vitejs/create-vite-app.git"
    },

    "license": "MIT",
    "scripts": {
        "dev": "webpack -w --env.ImHere",
        "buildw": "webpack --env.ImBuild",
        "buildr": "parcel build src/index.ts --target node --bundle-node-modules --no-source-maps",
        "buildc": "parcel build src/main.ts --target node --bundle-node-modules --no-source-maps",
        "buildr3": "parcel build src/index.ts --target node --bundle-node-modules --no-minify",
        "build": "rm -rf dist && yarn buildc && cp src/cli.js dist"
    },
    "dependencies": {
    },
    "devDependencies": {
        "@types/fs-extra": "^8.0.1",
        "@types/cheerio": "^0.22.14",
